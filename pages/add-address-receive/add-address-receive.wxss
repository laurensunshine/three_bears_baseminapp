@import "/asset/style/button.wxss";
page{
    background: #fbfafa;
}
.wrap{
    padding: 24rpx;
}
.address-list{
    padding: 0 28rpx;
    border-radius: 8px;
    background: #fff;
}
.address-item{
    display: flex;
    align-items: center;
    justify-content: space-between;
    align-items: center;
    padding: 30rpx 0;
   
}

.avatar image{
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
   
}
.name{
    font-size: 14px;
}
.phone-number{
    font-size: 12px;
    color: rgba(0,0,0,0.41);
}
.address-detail{
    width: 464rpx;
    font-size: 12px;
}
.edit{
    font-size: 12px;
    color: rgba(0,0,0,0.41);
    padding-left: 20rpx;
    border-left: 1px solid #fbfafa;
}